<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.lms.mapper.DepartmentMapper">

    <select id="selectDepartmentById" resultType="com.lms.entity.Department">
        select id
             , dept_name as deptName
          from departments
         where id = #{id}
    </select>
</mapper>